<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="simo\projetBundle\Entity\Singe" table="singe">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="age" type="integer" column="age" nullable="true">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="famille" type="string" column="famille" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="race" type="string" column="race" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nourriture" type="string" column="nourriture" length="50" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
